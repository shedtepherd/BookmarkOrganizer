{
    "manifest_version": 2,
    "name": "Bookmark Organizer",
    "version": "0.0.1",
    "default_local": "en",
    "description": "A chromium extension to file bookmarks into folders based on domain",
    "permissions": [
        "bookmarks"
    ],
    "browser_action": {
        "default_title": "popup",
        "default_popup": "popup/popup.html"
    }
}